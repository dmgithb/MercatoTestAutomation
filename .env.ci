# CI Environment Configuration
# This file provides default environment variables for CI environments
# where the local development BASE_URL may not be accessible

# Skip global setup in CI environments where local URLs are not accessible
SKIP_GLOBAL_SETUP=true

# CI Environment Indicator
CI=true

# Demo/Testing URL for CI environments where local development URLs are not accessible
# BASE_URL=https://demo.playwright.dev/

# Test Configuration
HEADLESS=true
TIMEOUT=30000
